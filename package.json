{
  "name": "plutin",
  "version": "1.1.1",
  "description": "Plutin lib",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js",
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./infra": {
      "import": "./dist/infra/index.js",
      "types": "./dist/infra/index.d.ts"
    }
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Pablo",
  "license": "ISC",
  "scripts": {
    "build": "tsup src --format cjs,esm --dts",
    "prepare:husky": "husky install",
    "eslint:fix": "npx eslint --fix src/**/*.ts",
    "prettier:format": "prettier --write src/**/*.ts",
    "lint-staged": "lint-staged",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "npm run build",
    "check:prepublish": "npm run eslint:fix && npm run prettier:format && npm run test",
    "prepublishOnly": "npm run check:prepublish"
  },
  "lint-staged": {
    "*.ts": [
      "npm run eslint:fix",
      "npm run prettier:format"
    ]
  },
  "devDependencies": {
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/js": "9.24.0",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.1",
    "@types/node": "20.0.0",
    "@vitest/coverage-v8": "1.0.0",
    "commitlint": "19.8.0",
    "dotenv": "16.5.0",
    "eslint": "9.24.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "prettier": "3.2.5",
    "tsup": "7.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.0",
    "unplugin-swc": "1.5.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "1.0.0"
  },
  "dependencies": {
    "@fastify/cors": "11.0.1",
    "@sentry/node": "9.13.0",
    "cors": "2.8.5",
    "discord-webhook-node": "1.1.8",
    "express": "5.1.0",
    "fastify": "5.2.2",
    "qs": "6.14.0",
    "reflect-metadata": "0.2.2",
    "zod": "3.24.3"
  }
}
